{
    "name": "audrey",
    "version": "0.0.0-1",
    "description": "",
    "author": "Kirill Khrushch√∂v",
    "private": false,
    "license": "MIT",
    "np": {
        "publish": false,
        "yarn": false,
        "anyBranch": true
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "ncc build src/main.ts --out dist",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:dev:local": "env-cmd nest start --debug 0.0.0.0:9229 --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist",
        "start:docker": "node index.js",
        "lint": "eslint \"{src,apps,libs,.e2e}/**/*.ts\" --fix",
        "test": "npm exec -- jest",
        "test:watch": "npm exec -- jest --watch --detectOpenHandles",
        "test:cov": "npm exec -- jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "docker:run": "docker compose up -d",
        "docker:run:rebuild": "docker compose build && docker compose up -d",
        "docker:clean": "docker system prune",
        "docker:clean:withVolumes": "docker system prune -a --volumes",
        "release": "npm exec -- np"
    },
    "dependencies": {
        "@distube/ytdl-core": "^4.14.4",
        "@nestjs/cache-manager": "^2.2.1",
        "@nestjs/common": "~10.3.3",
        "@nestjs/config": "~3.2.0",
        "@nestjs/core": "~10.3.3",
        "@nestjs/cqrs": "~10.2.7",
        "@nestjs/event-emitter": "~2.0.4",
        "@nestjs/microservices": "~10.3.3",
        "@nestjs/platform-express": "~10.3.3",
        "@nestjs/platform-socket.io": "~10.3.3",
        "@nestjs/platform-ws": "~10.3.3",
        "@nestjs/terminus": "~10.2.2",
        "@nestjs/websockets": "~10.3.3",
        "@vercel/ncc": "^0.38.1",
        "dayjs": "~1.11.6",
        "joi": "~17.9.2",
        "reflect-metadata": "~0.1.13",
        "rimraf": "~3.0.2",
        "rxjs": "~7.5.7",
        "uuid": "~9.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "~10.3.2",
        "@nestjs/schematics": "~10.1.1",
        "@nestjs/testing": "~10.3.3",
        "@types/cache-manager": "^4.0.1",
        "@types/express": "~4.17.13",
        "@types/jest": "29.5.12",
        "@types/node": "~16.11.66",
        "@types/supertest": "~2.0.11",
        "@typescript-eslint/eslint-plugin": "~6.6.0",
        "@typescript-eslint/parser": "~6.6.0",
        "browserify": "~17.0.0",
        "cross-env": "~7.0.3",
        "env-cmd": "~10.1.0",
        "eslint": "~8.46.0",
        "eslint-config-prettier": "~8.10.0",
        "eslint-plugin-prettier": "~4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "husky": "8.0.0",
        "jest": "29.7.0",
        "kafka-node": "~5.0.0",
        "lint-staged": "~12.5.0",
        "np": "~7.6.2",
        "prettier": "~2.8.8",
        "source-map-support": "~0.5.20",
        "supertest": "~6.3.3",
        "terser": "~5.19.2",
        "ts-jest": "29.1.2",
        "ts-loader": "~9.4.4",
        "ts-node": "~10.9.1",
        "tsconfig-paths": "4.1.0",
        "typescript": "~5.2.2"
    },
    "husky": {
        "hooks": {}
    },
    "lint-staged": {
        "*.{js,ts}": [
            "npm run lint"
        ]
    }
}
